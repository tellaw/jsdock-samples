{
	"alias" : "php",
	"image" : "tellaw77/lamp8.0",
	"sources" : "/var/www/html",
	"ports" : [
        {
            "host" : "80",
            "container" : "80"
        }
    ],
	"env" : {
		"mykey":"myvalue",
		"mykey2":"myvalue2"
	},
	"volumes" : [
        {
			"host" : "~/jsdock/conf/000-sf4-default.conf",
            "container" : "/etc/apache2/sites-available/000-default.conf",
			"conditions" : {
                "dirExists" : [
                    "web"
                ]
			}
		},
        {
			"host" : "~/jsdock/conf/000-sf5-default.conf",
            "container" : "/etc/apache2/sites-available/000-default.conf",
			"conditions" : {
                "dirExists" : [
                    "public"
                ]
			}
		}
    ]
	
}